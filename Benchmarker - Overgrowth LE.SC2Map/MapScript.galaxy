        Wait(1.0, c_timeGame);
    }
    gv_buildOrderStepLock = true;
    lv_currentStep = gv_currentBuildOrderStepCursor;
    lv_previousStep = (lv_currentStep - 1);
    lv_previousUnitStep = gf_GetStepIndexForPreviousUnitStep(lv_previousStep);
