        gf_LogTextMessage(gv_debugMenusEnabled, TextExpressionAssemble("Param/Expression/7EB63B80"));
        gf_SaveBenchmarkMenuConfigurationForBuild(gf_GetCurrentlySelectedBuildAsBankSectionKey());
    }
    else {
        TextExpressionSetToken("Param/Expression/2D716047", "A", StringToText(gf_GetCurrentlySelectedBuildAsBankSectionKey()));
        gf_LogTextMessage(gv_debugMenusEnabled, TextExpressionAssemble("Param/Expression/2D716047"));
    }
    TextExpressionSetToken("Param/Expression/34CF14C5", "A", IntToText(gv_currentlySelectedBuildNumber));
    gf_LogTextMessage(gv_debugMenusEnabled, TextExpressionAssemble("Param/Expression/34CF14C5"));
    gv_currentlySelectedBuildNumber = DialogControlGetSelectedItem(gv_buildSelectionPulldown, 1);
    BankValueSetFromInt(gv_benchmarkerBank, gf_GetCurrentlySelectedMatchupAsBankSectionKey(), gv_currentlySelectedBuildNumKey, gv_currentlySelectedBuildNumber);
    BankSave(gv_benchmarkerBank);
    gf_LoadBenchmarkMenuConfiguration(gf_GetCurrentlySelectedBuildAsBankSectionKey());
    return true;
}

