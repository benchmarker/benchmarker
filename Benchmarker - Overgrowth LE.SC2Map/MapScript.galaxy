    lv_alertIndex = gf_GetUpgradeGapAlertIndexForUpgrade(lp_upgrade);
    TimerStart(gv_upgradeGapAlerts[lv_alertIndex].lv_cooldownTimer, gv_neverexpires, false, c_timeGame);
