        lv_message = TextExpressionAssemble("Param/Expression/8534C2D6");
        gv_buildHintCursor += 1;
        UIDisplayMessage(PlayerGroupAll(), c_messageAreaDirective, lv_message);
        Wait(0.5, c_timeGame);
        UIClearMessages(PlayerGroupAll(), c_messageAreaDirective);
        Wait(0.5, c_timeGame);
        UIDisplayMessage(PlayerGroupAll(), c_messageAreaDirective, lv_message);
        Wait(0.5, c_timeGame);
        UIClearMessages(PlayerGroupAll(), c_messageAreaDirective);
        Wait(0.5, c_timeGame);
