    gt_HatcheryLarvaSpawnFix = TriggerCreate("gt_HatcheryLarvaSpawnFix_Func");
    TriggerAddEventUnitRegion(gt_HatcheryLarvaSpawnFix, null, RegionEntireMap(), true);
}

